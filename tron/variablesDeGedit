#!/bin/bash
# [Gedit Tool]
# Name=Variables
# Input=selection-document
# Output=output-panel
# Applicability=all
# Save-files=nothing
# Shortcut=<Primary><Shift><Super>v
#LISTA LAS VARIABLES INTERNAS DE GEDIT


echo 'GEDIT_CURRENT_DOCUMENT_DIR' $GEDIT_CURRENT_DOCUMENT_DIR

echo 'GEDIT_CURRENT_DOCUMENT_NAME' $GEDIT_CURRENT_DOCUMENT_NAME

echo 'GEDIT_CURRENT_DOCUMENT_PATH' $GEDIT_CURRENT_DOCUMENT_PATH

echo 'GEDIT_CURRENT_DOCUMENT_SCHEME' $GEDIT_CURRENT_DOCUMENT_SCHEME

echo 'GEDIT_CURRENT_DOCUMENT_TYPE' $GEDIT_CURRENT_DOCUMENT_TYPE

echo 'GEDIT_CURRENT_DOCUMENT_URI' $GEDIT_CURRENT_DOCUMENT_URI

echo 'GEDIT_CURRENT_DOCUMENT_LANGUAGE' $GEDIT_CURRENT_DOCUMENT_LANGUAGE

echo 'GEDIT_CURRENT_LINE' $GEDIT_CURRENT_LINE

echo 'GEDIT_CURRENT_LINE_NUMBER' $GEDIT_CURRENT_LINE_NUMBER

echo 'GEDIT_CURRENT_WORD' $GEDIT_CURRENT_WORD

echo 'GEDIT_CWD' $GEDIT_CWD

echo 'GEDIT_DOCUMENTS_PATH' $GEDIT_DOCUMENTS_PATH

echo 'GEDIT_DOCUMENTS_URI' $GEDIT_DOCUMENTS_URI

echo 'GEDIT_FILE_BROWSER_ROOT' $GEDIT_FILE_BROWSER_ROOT

echo 'GEDIT_SELECTED_TEXT' "$GEDIT_SELECTED_TEXT" 
